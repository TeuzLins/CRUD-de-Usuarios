{
  "devDependencies": {
    "concurrently": "^9.2.1",
    "jsdom": "^27.1.0",
    "json-server": "^0.17.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.6"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:client": "vite",
    "dev:api": "json-server --watch db.json --port 3000 --delay 800",
    "dev:all": "concurrently -n web,api -c blue,green \"npm run dev:client\" \"npm run dev:api\"",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest"
  }
}
